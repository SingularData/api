  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SingularData/dataset-api/issues"
  },
  "homepage": "https://github.com/SingularData/dataset-api#readme",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/dotenv": "^4.0.2",
    "@types/ini": "^1.3.29",
    "@types/mocha": "^2.2.48",
    "@types/node-fetch": "^1.6.7",
    "chai": "^4.1.2",
    "copy": "^0.3.1",
    "dotenv": "^5.0.0",
    "husky": "^0.14.3",
    "ini": "^1.3.5",
    "lint-staged": "^6.1.1",
    "mocha": "^5.0.1",
    "node-fetch": "^2.0.0",
    "nyc": "^11.5.0",
    "prettier": "^1.10.2",
    "serverless": "^1.26.0",
    "serverless-dotenv-plugin": "^1.0.2",
    "serverless-offline": "^3.18.0",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.16.8",
    "@types/config": "0.0.33",
    "@types/express": "^4.11.1",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.104",
    "@types/lodash.assign": "^4.2.3",
    "@types/node": "^9.4.6",
    "aws-sdk": "^2.197.0",
    "body-parser": "^1.18.2",
    "elasticsearch": "^14.1.0",
